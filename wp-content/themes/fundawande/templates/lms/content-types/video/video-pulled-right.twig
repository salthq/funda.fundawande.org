<!-- Content Type: Video Pulled Right -->
<div class="container my-5">
    <div class="row">
        <div class="col-12 col-md-6 pt-4">
            {{content.text}}
        </div>
        <div class="col-12 col-md-6 pt-4">
            {# Remove whitespace from beginning and end of the file name before appending to media url #}
            {% set video_file_name = content.video_file_name | trim %}
            {{ mixin.videoPlayer(media_url~video_file_name, content.holding_image, module_number) }}
        </div>
    </div>
</div>